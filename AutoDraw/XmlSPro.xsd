										<xs:element name="wayPointType" type="xs:string"></xs:element>
										<xs:element name="wayPointLocation" type="xs:string"></xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="connection">
								
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:attribute name="ProjectTime" type="xs:date" />
	
